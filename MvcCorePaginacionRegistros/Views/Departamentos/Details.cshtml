@{
    Departamento depart = ViewData["DEPART"] as Departamento;
}

<h1>Detalles Departamento @depart.DeptNo</h1>

<ul class="list-group mt-3">
    <li class="list-group-item list-group-item-info">
        Nombre: @depart.Nombre
    </li>
    <li class="list-group-item list-group-item-info">
        Localidad: @depart.Localidad
    </li>
</ul>
